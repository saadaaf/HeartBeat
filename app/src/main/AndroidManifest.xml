<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.followersandleaders">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="com.android.vending.BILLING" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="com.android.vending.CHECK_LICENSE" />


    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.followersandleaders.Activitys.OneTimePage" android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.followersandleaders.Activitys.HomeActivity" android:screenOrientation="portrait"/>
        <activity android:name="com.followersandleaders.Activitys.UserProfile" android:screenOrientation="portrait"/>
        <activity android:name="com.followersandleaders.Activitys.BuyCoins" android:screenOrientation="portrait"/>
        <activity android:name="com.followersandleaders.Activitys.GetStart" android:screenOrientation="portrait"/>
        <activity android:name="com.followersandleaders.Activitys.SocialPage" android:screenOrientation="portrait"/>
        <activity android:name="com.followersandleaders.Activitys.MorePage" android:screenOrientation="portrait"/>
        <activity android:name="com.followersandleaders.Activitys.AddAccount" android:screenOrientation="portrait"/>
        <activity android:name="com.followersandleaders.Activitys.AddAccount2" android:screenOrientation="portrait"/>
        <activity android:name="com.followersandleaders.Activitys.SubmitForLike" android:screenOrientation="portrait"></activity>
    </application>

</manifest>
